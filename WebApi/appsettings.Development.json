{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${PGHOST};Port=5432;Database=${PGDATABASE};Username=${PGUSER};Password=${PGPASSWORD};"
  },
  "Jwt": {
    "Key": "${Jwt__Key}",
    "Issuer": "${Jwt__Issuer}",
    "Audience": "${Jwt__Audience}",
    "ExpireMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}